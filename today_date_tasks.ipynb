   "source": [
    "from datetime import date\n",
    "\n",
    "# Get today's date\n",
    "today = date.today()\n",
    "\n",
    "# Convert the date to a string in ISO format\n",
    "today_str = today.isoformat()\n",
    "\n",
    "# Write the date string to a file\n",
    "with open(\"today.txt\", \"w\") as file:\n",
    "    file.write(today_str)"
   ]
  }
